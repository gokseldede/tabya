    <script>
        $(document)
            .ready(function () {
                $('#dataTables-example').DataTable({
                    responsive: true
                });

                //CKEDITOR.replace(content);


                $('#Il').on('change', function () {
                    GetIlceler(this.value);
                });
                $('#IlceId').on('change', function () {
                    GetSemtler(this.value);
                });

                function GetIlceler(ilId) {
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("GetirIlceler", "Home", new {area = ""})',
                        data: { ilId: ilId },
                        dataType: "json",
                        success: function (ilceler) {
                            $("#IlceId").html("");
                            $.each(ilceler, function (index, ilce) {
                                $("#IlceId").append("<option value=" + ilce.Id + ($("#IlceId").data("id") === ilce.Id ? " Selected='selected'>" : ">") + ilce.Value + "</option>");
                            });
                            $("#IlceId").trigger("change");
                        }
                    });
                }

                function GetSemtler(ilceId) {
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("GetirSemtler", "Home", new {area = ""})',
                        data: { ilceId: ilceId },
                        dataType: "json",
                        success: function (semtler) {
                            $("#SemtId").html("");
                            $.each(semtler, function (index, semt) {
